<%# <%= content_for :page_scripts do %>
  <%# <%= javascript_include_tag "expenses"%>
    <%# <% end %>
      <div class="card text-center border-info mb-3">
        <div class="card-header card-title text-info">
          <h1>Import CSV</h1>
        </div>
        <div class="card-body">
          <%= form_tag(import_csv_expenses_path, method: :post, multipart: true) do %>
          <div class="row">
            <div class="col-md-6 offset-md-3">
              <div class="input-group">
                <div class="custom-file">
                  <input type="file" class="custom-file-input" id="csv_file" ,="," name="csv_file" accept="text/csv">
                  <label class="custom-file-label" for="csv_file">Choose file...</label>
                </div>
              </div><br>
              <div class="form-group">
                <button class="btn btn-outline-info my-2 my-sm-0 mr-sm-2" type="submit" title="Upload">
                  <i class="fa fa-upload" aria-hidden="true"></i>
                </button>
                <%= link_to download_csv_expenses_path(format: "csv"), class: "btn btn-outline-primary my-2 my-sm-0 mr-sm-2", title: 'Download layout' do %>
                  <i class="fa fa-download" aria-hidden="true"></i>
                <% end %>
              </div>
            </div>
          </div>
        <% end %>
      </div>
    </div>
